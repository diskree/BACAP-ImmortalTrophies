  "authors": [
    "${author}"
  ],
  "contact": {
    "sources": "${repoUrl}"
  },
  "license": "MIT",
  "icon": "assets/immortaltrophies/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.diskree.immortaltrophies.ImmortalTrophies"
    ]
  },
  "mixins": [
    "immortaltrophies.mixins.json"
  ],
  "depends": {
    "java": ">=${minJavaVersion}",
    "minecraft": ">=${minMinecraftVersion}",
    "fabricloader": ">=${minLoaderVersion}",
    "fabric-api": "*"
